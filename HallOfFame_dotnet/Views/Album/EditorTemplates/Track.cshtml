@model HallOfFame_dotnet.Models.Track

<div class="track">
    <div class="editor-label">
        @Html.LabelFor(t => t.Number)
    </div>
    <div class="editor-field">
        @Html.EditorFor(t => t.Number)
    </div>

    <div class="editor-label">
        @Html.LabelFor(t => t.Name)
    </div>
    <div class="editor-field">
        @Html.EditorFor(t => t.Name)
    </div>

    <div class="editor-label">
        @Html.LabelFor(t => t.Duration)
    </div>
    <div class="editor-field">
        @Html.EditorFor(t => t.Duration)
    </div>
</div>

